<div id="<%= dom_id report %>">
  <p>
    <strong>Title:</strong>
    <%= report.title %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= report.body %>
  </p>

  <p>
    <strong>User:</strong>
    <%= current_user_name(report.user) %>
  </p>

  <p>
    <strong>Posted at:</strong>
    <%= report.created_at %>
  </p>
</div>
